   "source": [
    "from jgh.Interface import Interface\n",
    "import glob\n",
    "\n",
    "repo = Interface()\n",
    "\n",
    "for path in glob.glob('*'):\n",
    "    if path == 'raw_reads':\n",
    "        continue\n",
    "    if path == 'jgh':\n",
    "        continue\n",
    "    repo.evaladd(path)\n",
    "    repo.commitpush(path)"
   ]
